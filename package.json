{
  "name": "clockify-mcp",
  "version": "0.0.1",
  "description": "A MCP Server for Clockify",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "license:check": "license-checker --onlyAllow 'MIT;Apache-2.0;ISC;BSD-2-Clause;BSD-3-Clause;0BSD;CC0-1.0;Unlicense' --production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimwenigerkind/clockify-mcp.git"
  },
  "keywords": [
    "mcp",
    "clockify"
  ],
  "author": "Wim Wenigerkind <wim.wenigerkind@heptacom.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wimwenigerkind/clockify-mcp/issues"
  },
  "homepage": "https://github.com/wimwenigerkind/clockify-mcp#readme",
  "bin": {
    "clockify-mcp": "build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.7.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "license-checker": "^25.0.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
